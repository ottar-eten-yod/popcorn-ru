{
    "name": "popcorn/popcorn",
    "description": "Popcorn Time API",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-anitomy": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "artprima/prometheus-metrics-bundle": "^1.14",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "enqueue/dbal": "^0.10",
        "enqueue/enqueue-bundle": "^0.10",
        "friendsofsymfony/elastica-bundle": "dev-master",
        "guzzlehttp/guzzle": "^7.3",
        "long-running/long-running": "^1.0",
        "nyholm/psr7": "^1.4",
        "php-http/cache-plugin": "^1.7",
        "php-tmdb/symfony": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "ramsey/uuid-doctrine": "^1.5",
        "s1njar/kitsu-php": "dev-fix",
        "sensio/framework-extra-bundle": "^5.5",
        "sentry/sentry-symfony": "^4.3",
        "symfony/console": "^5.1",
        "symfony/css-selector": "^5.1",
        "symfony/dom-crawler": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/flex": "^1.3",
        "symfony/framework-bundle": "^5.1",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/proxy-manager-bridge": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "roave/security-advisories": "dev-master",
        "symfony/maker-bundle": "^1.15"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/php-tmdb/symfony"
        },
        {
            "type": "git",
            "url": "https://github.com/Ruin0x11/kitsu-php"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4"
        }
    }
}
